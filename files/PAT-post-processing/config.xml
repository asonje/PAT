<post-config>
	<pdf enable='yes'>
		<average-cpu>yes</average-cpu>
		<average-disk>yes</average-disk>
		<average-net>yes</average-net>
		<average-perf>yes</average-perf>
		<average-memory>yes</average-memory>
		<all-nodes-cpu>no</all-nodes-cpu>
		<all-nodes-disk>no</all-nodes-disk>
		<all-nodes-net>no</all-nodes-net>
		<all-nodes-perf>no</all-nodes-perf>
		<all-nodes-memory>no</all-nodes-memory>
	</pdf>
	<excel enable='yes'>
		<cpu>yes</cpu>
		<disk>yes</disk>
		<net>yes</net>
		<perf>yes</perf>
		<memory>yes</memory>
	</excel>
	<csv-format enable='yes'>
		<csv-cpu>yes</csv-cpu>
		<csv-disk>yes</csv-disk>
		<csv-net>yes</csv-net>
		<csv-perf>yes</csv-perf>
		<csv-memory>yes</csv-memory>
	</csv-format>
    <source>/foo/bar/instruments/</source>
    <exclude-node>exclude-node</exclude-node>
</post-config>
